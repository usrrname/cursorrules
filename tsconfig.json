{
    "compilerOptions": {
        "paths": {
            "@/*": [
                "./*"
            ],
            "@/docs/*": [
                "./docs/*"
            ]
        },
        "target": "es2022",
        "module": "esnext",
        "moduleResolution": "node",
        "rootDir": "./",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "strict": true,
        "useUnknownInCatchVariables": false,
        "lib": ["es2022", "dom"],
        "skipLibCheck": true
    }
}